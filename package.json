  "devDependencies": {
    "@floffah/build": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "all-contributors-cli": "^6.20.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-workspaces": "^0.6.2",
    "lerna": "^4.0.0"
  },
  "version": "0.0.0"
}
