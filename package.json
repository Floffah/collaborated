{
  "name": "collaborated",
  "workspaces": [
    "packages/*",
    "test/*"
  ],
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "docs": "yarn buildts && yarn workspace @collaborated/web docs",
